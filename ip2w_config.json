{
  "IPINFO_TOKEN": "",
  "OPENWEATHERMAP_TOKEN": "",
  "LOGGER_CONF": {
    "version": 1,
    "formatters": {
      "default": {
        "format": "[%(asctime)s] %(levelname).1s %(message)s",
        "datefmt": "%Y.%m.%d %H:%M:%S"
      }
    },
    "handlers": {
      "file": {
        "level": "INFO",
        "formatter": "default",
        "class": "logging.handlers.RotatingFileHandler",
        "filename": "/var/log/ip2w/ip2w.log"
      }
    },
    "loggers": {
      "default": {
        "level": "INFO",
        "handlers": [
          "file"
        ]
      }
    }
  },
  "REQUEST_TIMEOUT": 15,
  "REQUEST_MAX_RETRY": 5
}
